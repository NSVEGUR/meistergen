	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"eslint-config-custom": "workspace:*",
		"svelte": "^3.54.0",
		"sass": "^1.58.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@sentry/node": "^7.38.0",
		"@sentry/tracing": "^7.38.0"
	}
